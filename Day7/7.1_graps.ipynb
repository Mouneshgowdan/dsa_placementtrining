        "# Social network graph\n",
        "social_network = {\n",
        "    'Alice': ['Bob', 'Charlie'],\n",
        "    'Bob': ['Alice', 'David'],\n",
        "    'Charlie': ['Alice', 'Eve'],\n",
        "    'David': ['Bob'],\n",
        "    'Eve': ['Charlie']\n",
        "}\n",
        "\n",
        "def suggest_friends(graph, person):\n",
        "    friends = set(graph[person])\n",
        "    suggestions = set()\n",
        "\n",
        "    for friend in friends:\n",
        "        for fof in graph[friend]:  # friend of friend\n",
        "            if fof != person and fof not in friends:\n",
        "                suggestions.add(fof)\n",
        "\n",
        "    return suggestions\n",
        "\n",
        "print(\"Friend suggestions for Alice ðŸ‘¥:\", suggest_friends(social_network, 'Alice'))\n"
